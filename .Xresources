*customization: -color

*background: #282828
*foreground: #ebdbb2

URxvt*scrollBar: 0
URxvt*cursorBlink: 1
URxvt.font: xft:Hack:size=14:antialias=false, xft:FontAwesome:size=14, xft:Knack Nerd Font:size=14
URxvt.letterSpace: -2
URxvt.depth: 24

URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,vtwheel

